<?xml version='1.0' encoding='utf-8'?>
<s:SparkSkin implements='mx.core.IRectangularBorder'
    xmlns:fx='http://ns.adobe.com/mxml/2009'
    xmlns:s='library://ns.adobe.com/flex/spark'>
  <fx:Script source='BorderSkin.mxml.as'/>
  <s:Rect id='dropShadow' bottom='-1' left='1' right='-1' top='1'>
    <s:fill><s:SolidColor alpha='.12' color='0x000000'/></s:fill>
  </s:Rect>
  <s:Rect id='border' bottom='0' left='0' right='0' top='0'>
    <s:fill><s:SolidColor id='backgroundFill' color='0xffffff'/></s:fill>
    <s:stroke>
      <s:SolidColorStroke id='borderStroke' color='0x000000'/>
    </s:stroke>
  </s:Rect>
</s:SparkSkin>
